<div class="outfitcontainer container">
    {% for outfit in outfits %}
        <div class="outfitcard card col-5 mt-5">
            <img class="card-img-top" src="{{ asset('uploads/outfits/{{ outfit.picture }}') }}" alt="{{ outfit.name }}">
            <div class="card-body">
                <p class="card-text">{{ outfit.name }} :
            </div>
        </div>
    {% endfor %}
</div>
