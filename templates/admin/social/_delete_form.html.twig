<form method="post" action="{{ path('social_delete', {'id': social.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer cet objet ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ social.id) }}">
    <button class="btn mb-3">Supprimer</button>
</form>
